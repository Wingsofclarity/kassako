.SUFFIXES: .java .class
JC=javac

comp: Queue.class
	@echo Compiling.
	@$(JC) Simulator.java Store.java Simulation.java Customer.java CustomerTest.java Register.java Queue.java

run: comp
	@echo Running.
	@java Simulator

clean:
	@echo Cleaning.
	@rm -f *.class
	@rm -f *.#
	@rm -f *.*#
	@rm -f .*~

Queue.class: Queue.java
	@$(JC) $^




cr: clean comp run
